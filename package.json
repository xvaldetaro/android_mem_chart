{
  "name": "android_mem_chart",
  "version": "1.1.3",
  "description": "A simple graph showing android memory usage",
  "bin": {
    "androidmc": "src/server/androidmc"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run build-server",
    "lint": "vue-cli-service lint",
    "bin": "ts-node -r tsconfig-paths/register -O '{\"module\": \"commonjs\"}' src/server/androidmc",
    "build-server": "rm -rf bin && tsc --outDir temp -p ./tsconfig.json -m commonjs && mkdir bin && mv -f temp/server/* bin && rm -rf temp && cp src/server/androidmc bin/ && chmod +x bin/androidmc"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@types/finalhandler": "^1.1.0",
    "@types/js-base64": "^2.3.1",
    "@types/opn": "^5.5.0",
    "@types/ramda": "^0.26.8",
    "@types/semver": "^6.0.0",
    "@types/websocket": "^0.0.40",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "chart.js": "^2.8.0",
    "core-js": "^2.6.7",
    "finalhandler": "^1.1.2",
    "js-base64": "^2.5.1",
    "node-sass": "^4.12.0",
    "opn": "^6.0.0",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.2",
    "sass-loader": "^7.1.0",
    "semver": "^6.1.1",
    "serve-static": "^1.14.1",
    "tar": ">=4.4.2",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.3",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.6",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.1.1",
    "websocket": "^1.0.28"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
